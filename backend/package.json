{
  "name": "cpaaide-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Multi-Tenant AI Document Management SaaS Backend",
  "main": "server.js",
  "scripts": {
    "dev": "node --watch server.js",
    "start": "node server.js",
    "prisma:generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./src/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./src/prisma/schema.prisma",
    "prisma:seed": "node src/prisma/seed.js",
    "init-tenant-folders": "node scripts/initialize-tenant-folders.js"
  },
  "keywords": [
    "express",
    "prisma",
    "postgresql",
    "multi-tenant",
    "ai",
    "document-management"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "otp-generator": "^4.0.1",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "prisma": "^5.8.0"
  }
}